package dependencies;

import java.awt.Rectangle;
import java.util.LinkedList;
import java.util.List;

import AbstractObjects.Entities;

public class QuadTree {

	private final static int  maxObjects = 10;
	private final static int maxDepth = 5;
	
	private int level;
	private List<Entities> objects;
	
	//bounds
	private int originX;
	private int originY;
	private int width;
	private int height;
	 
	//nodes
	private QuadTree topLeft;
	private QuadTree topRight;
	private QuadTree bottomLeft;
	private QuadTree bottomRight;

	//constructor
	public QuadTree(int pLevel,int newX,int newY,int newWidth,int newHeight) 
	{
		level = pLevel;
		objects = new LinkedList();
		originX = newX;
		originY = newY;
		width = newWidth;
		height = newHeight;
	}
	
	
	  
}
